{
  "version": "1.0.0",
  "tenants": [
    {
      "id": "amazon",
      "name": "Amazon Marketplace",
      "baseUrl": "https://webservices.amazon.com/paapi5",
      "apiKey": "demo-mode-no-key-required",
      "apiKeyHeader": "X-Amz-Access-Token",
      "responseFormat": "nested",
      "recommendationLimit": 12,
      "personalizationBoost": 1.8,
      "maxDiscount": 0.15,
      "pricingStrategy": "optimize-margin",
      "categoryWeights": {
        "electronics": 1.5,
        "books": 1.3,
        "home": 1.2,
        "clothing": 1.1,
        "sports": 1.0
      },
      "endpoints": [
        {
          "name": "product-search",
          "pattern": "^searchitems$",
          "cacheable": true,
          "cacheTTL": 600,
          "personalization": {
            "enabled": true,
            "type": "product-listing"
          }
        },
        {
          "name": "product-details",
          "pattern": "^getitems$",
          "cacheable": true,
          "cacheTTL": 1800,
          "personalization": {
            "enabled": true,
            "type": "product-recommendations"
          }
        },
        {
          "name": "browse-nodes",
          "pattern": "^getbrowsenodes$",
          "cacheable": true,
          "cacheTTL": 3600,
          "personalization": {
            "enabled": false,
            "type": "none"
          }
        }
      ],
      "segmentationRules": {
        "purchaseThreshold": 10,
        "valueThreshold": 1000,
        "frequencyThreshold": 0.3
      },
      "contentRules": {
        "purchaseIntent": {
          "callToAction": "Buy Now",
          "urgency": "Limited time offer!"
        },
        "mobile": {
          "layout": "list",
          "imageSize": "large"
        },
        "darkTheme": {
          "enabled": true
        }
      },
      "rateLimits": {
        "requestsPerSecond": 10,
        "requestsPerMinute": 300,
        "requestsPerHour": 8640
      },
      "headers": {
        "X-Forwarded-By": "edge-ai-proxy",
        "X-Proxy-Version": "2.0.0",
        "User-Agent": "EdgeAIProxy/2.0"
      }
    },
    {
      "id": "rakuten",
      "name": "Rakuten Marketplace",
      "baseUrl": "https://app.rakuten.co.jp/services/api",
      "apiKey": "demo-mode-no-key-required",
      "apiKeyHeader": "applicationId",
      "responseFormat": "nested",
      "recommendationLimit": 15,
      "personalizationBoost": 1.6,
      "maxDiscount": 0.25,
      "pricingStrategy": "optimize-conversion",
      "categoryWeights": {
        "electronics": 1.4,
        "fashion": 1.3,
        "beauty": 1.2,
        "home": 1.1,
        "sports": 1.0
      },
      "endpoints": [
        {
          "name": "product-search",
          "pattern": "^IchibaItem/Search/20170706$",
          "cacheable": true,
          "cacheTTL": 300,
          "personalization": {
            "enabled": true,
            "type": "product-listing"
          }
        },
        {
          "name": "product-ranking",
          "pattern": "^IchibaItem/Ranking/20170628$",
          "cacheable": true,
          "cacheTTL": 900,
          "personalization": {
            "enabled": true,
            "type": "product-recommendations"
          }
        },
        {
          "name": "genre-search",
          "pattern": "^IchibaGenre/Search/20140222$",
          "cacheable": true,
          "cacheTTL": 1800,
          "personalization": {
            "enabled": true,
            "type": "category-personalization"
          }
        },
        {
          "name": "recipe-search",
          "pattern": "^Recipe/CategoryList/20170426$",
          "cacheable": true,
          "cacheTTL": 3600,
          "personalization": {
            "enabled": true,
            "type": "content-personalization"
          }
        }
      ],
      "segmentationRules": {
        "purchaseThreshold": 8,
        "valueThreshold": 50000,
        "frequencyThreshold": 0.4
      },
      "contentRules": {
        "purchaseIntent": {
          "callToAction": "今すぐ購入",
          "urgency": "期間限定セール中！"
        },
        "mobile": {
          "layout": "grid",
          "imageSize": "medium"
        },
        "darkTheme": {
          "enabled": false
        }
      },
      "rateLimits": {
        "requestsPerSecond": 20,
        "requestsPerMinute": 1000,
        "requestsPerHour": 20000
      },
      "headers": {
        "X-Forwarded-By": "edge-ai-proxy",
        "X-Proxy-Version": "2.0.0"
      }
    },
    {
      "id": "zalando",
      "name": "Zalando Fashion Marketplace",
      "baseUrl": "https://api.zalando.com/v1",
      "apiKey": "demo-mode-no-key-required",
      "apiKeyHeader": "Authorization",
      "responseFormat": "standard",
      "recommendationLimit": 20,
      "personalizationBoost": 2.0,
      "maxDiscount": 0.35,
      "pricingStrategy": "optimize-conversion",
      "categoryWeights": {
        "clothing": 1.8,
        "shoes": 1.6,
        "accessories": 1.4,
        "beauty": 1.3,
        "sport": 1.2
      },
      "endpoints": [
        {
          "name": "product-search",
          "pattern": "^articles$",
          "cacheable": true,
          "cacheTTL": 180,
          "personalization": {
            "enabled": true,
            "type": "fashion-recommendations"
          }
        },
        {
          "name": "product-details",
          "pattern": "^articles/[^/]+$",
          "cacheable": true,
          "cacheTTL": 600,
          "personalization": {
            "enabled": true,
            "type": "product-recommendations"
          }
        },
        {
          "name": "categories",
          "pattern": "^categories$",
          "cacheable": true,
          "cacheTTL": 1800,
          "personalization": {
            "enabled": true,
            "type": "category-personalization"
          }
        },
        {
          "name": "brands",
          "pattern": "^brands$",
          "cacheable": true,
          "cacheTTL": 3600,
          "personalization": {
            "enabled": true,
            "type": "brand-personalization"
          }
        },
        {
          "name": "size-advice",
          "pattern": "^size-advice/[^/]+$",
          "cacheable": true,
          "cacheTTL": 7200,
          "personalization": {
            "enabled": true,
            "type": "size-personalization"
          }
        }
      ],
      "segmentationRules": {
        "purchaseThreshold": 6,
        "valueThreshold": 300,
        "frequencyThreshold": 0.6
      },
      "contentRules": {
        "purchaseIntent": {
          "callToAction": "In den Warenkorb",
          "urgency": "Nur noch wenige auf Lager!"
        },
        "mobile": {
          "layout": "masonry",
          "imageSize": "large"
        },
        "darkTheme": {
          "enabled": true
        }
      },
      "rateLimits": {
        "requestsPerSecond": 50,
        "requestsPerMinute": 2000,
        "requestsPerHour": 50000
      },
      "headers": {
        "X-Forwarded-By": "edge-ai-proxy",
        "X-Proxy-Version": "2.0.0",
        "Accept-Language": "en-US,de-DE,fr-FR"
      }
    }
  ]
}
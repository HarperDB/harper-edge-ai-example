{
  "name": "edge-ai-proxy",
  "version": "3.0.0",
  "description": "Edge AI Proxy Service - Transparent caching and personalization layer for e-commerce APIs",
  "type": "module",
  "main": "proxy-server/index.js",
  "scripts": {
    "start": "node proxy-server/index.js",
    "start:proxy": "node proxy-server/index.js",
    "dev": "nodemon proxy-server/index.js",
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "test": "npm run test-models && npm run validate-harper",
    "test:models": "node scripts/test-models.js",
    "test:harper": "node harper-components/tests/basic-validation.js",
    "test:all": "npm run test-models && npm run validate-harper",
    "setup-models": "node scripts/setup-models.js",
    "setup-advanced-models": "node scripts/setup-advanced-models.js",
    "download-models": "node scripts/download-models.js",
    "test-models": "node scripts/test-models.js",
    "validate-harper": "node harper-components/tests/basic-validation.js"
  },
  "dependencies": {
    "@tensorflow-models/face-landmarks-detection": "^1.0.6",
    "@tensorflow-models/handpose": "^0.1.0",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow-models/posenet": "^2.2.2",
    "@tensorflow-models/qna": "^1.0.2",
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "harperdb": "^4.0.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "lru-cache": "^10.4.3",
    "semver": "^7.5.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "license": "Apache-2.0"
}
